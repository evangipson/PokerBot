{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SassCompiler": {
    "Source": "./Styles/main.scss",
    "Target": "wwwroot/style/main.min.css",
    "Arguments": "--style=compressed",
    "GenerateScopedCss": false,
    "IncludePaths": [ "base" ]
  }
}
